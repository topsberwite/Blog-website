<%- include("partials/header.ejs") %>


<h1>Home</h1>
<p>
  <%= startingContent %>
</p>


<% posts.forEach((post) => { %>
  <div>
  
    <h2>
      <%= post.title %>
    </h2>
    <p>
      <%= post.content.substring(0, 99) %> ...
      <a href="/posts/<%= post._id %>" id="links">Read More</a>
    </p>

  </div>
<% }); %>


<%- include("partials/footer.ejs") %>